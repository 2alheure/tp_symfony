{% include 'parties/header.html.twig' %}
<h1>Les classes</h1>

<table class="table">
    <thead>
        <tr>
            <th>Nom</th>
            <th>Niveau</th>
            <th>Prof. principal</th>
        </tr>
    </thead>
    <tbody>
        {% for classe in classes %}
        <tr>
            <td>{{ classe.nom }}</td>
            <td>{{ classe.niveau }}</td>
            <td>{{ classe.profPrincipal.nomEtPrenom }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
{% include 'parties/footer.html.twig' %}